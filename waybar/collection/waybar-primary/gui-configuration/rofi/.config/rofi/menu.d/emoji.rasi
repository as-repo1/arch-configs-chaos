@import "./config.rasi"

@theme "/dev/null"
@import "../themes/colors.rasi"
@import "../menu-conf.rasi"

window {
  location: center;
  width: 30%;
  x-offset: 0%;
  y-offset: 0;
  height: 60%;
}

listview, element, element selected, element-text, element-icon {
  padding: 4px;
}

#element selected {
  background-color: @al;
  padding: 0px;
  margin: 0px;
}
